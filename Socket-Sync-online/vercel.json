{
    "version": 2,
    "rewrites": [
        {
            "source": "/",
            "destination": "/frontend/pages/login.html"
        },
        {
            "source": "/login",
            "destination": "/frontend/pages/login.html"
        },
        {
            "source": "/signup",
            "destination": "/frontend/pages/signup.html"
        },
        {
            "source": "/chat",
            "destination": "/frontend/pages/chat.html"
        },
        {
            "source": "/oauth-callback",
            "destination": "/frontend/pages/oauth-callback.html"
        },
        {
            "source": "/css/:path*",
            "destination": "/frontend/css/:path*"
        },
        {
            "source": "/js/:path*",
            "destination": "/frontend/js/:path*"
        },
        {
            "source": "/material/:path*",
            "destination": "/frontend/material/:path*"
        },
        {
            "source": "/pages/:path*",
            "destination": "/frontend/pages/:path*"
        }
    ]
}