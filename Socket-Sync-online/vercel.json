{
    "version": 2,
    "rewrites": [
        {
            "source": "/",
            "destination": "/frontend/pages/login.html"
        },
        {
            "source": "/login",
            "destination": "/frontend/pages/login.html"
        },
        {
            "source": "/signup",
            "destination": "/frontend/pages/signup.html"
        },
        {
            "source": "/chat",
            "destination": "/frontend/pages/chat.html"
        },
        {
            "source": "/oauth-callback",
            "destination": "/frontend/pages/oauth-callback.html"
        },
        {
            "source": "/js/(.*)",
            "destination": "/frontend/js/$1"
        },
        {
            "source": "/css/(.*)",
            "destination": "/frontend/css/$1"
        },
        {
            "source": "/material/(.*)",
            "destination": "/frontend/material/$1"
        },
        {
            "source": "/pages/(.*)",
            "destination": "/frontend/pages/$1"
        }
    ]
}